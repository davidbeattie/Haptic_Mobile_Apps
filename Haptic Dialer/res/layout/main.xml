<?xml version="1.0" encoding="utf-8"?>
<TabHost xmlns:android="http://schemas.android.com/apk/res/android"
         android:id="@+id/tabhost"
         android:layout_width="fill_parent"
         android:layout_height="fill_parent">
      <TabWidget
         android:id="@android:id/tabs"
         android:layout_width="fill_parent"
         android:layout_height="wrap_content"/>
      <FrameLayout
                android:id="@android:id/tabcontent"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:paddingTop="70px">
                 <LinearLayout
                      android:id="@+id/content"
                      android:orientation="vertical"
                      android:layout_width="fill_parent"
                      android:layout_height="fill_parent">
                 </LinearLayout>
				 <ListView
         			  android:id = "@+id/list"
         			  android:layout_width="fill_parent"
         			  android:layout_height="wrap_content">
         		 </ListView>
         		 <AbsoluteLayout 
					  android:layout_height="fill_parent" 
					  xmlns:android="http://schemas.android.com/apk/res/android" 
					  android:layout_width="fill_parent" 
					  android:id="@+id/dialertab">

							<ImageButton 
								android:layout_width="60px"
								android:id="@+id/add" 
								android:textStyle="bold"
								android:src="@drawable/add_contact_grey"
								android:layout_x="25px" 
								android:layout_y="277px" 
								android:textSize="18sp"
								android:layout_height="70px">
							</ImageButton>
							<Button 
								android:id="@+id/call" 
								android:text="CALL" 
								android:textSize="18sp"
								android:textStyle="bold"
								android:layout_width="150px" 
								android:layout_x="87.5px" 
								android:layout_y="277px" 
								android:layout_height="70px">
							</Button>
							<ImageButton 
								android:layout_width="60px" 
								android:id="@+id/back" 
								android:text="Back"
								android:src="@drawable/backgrey" 
								android:textStyle="bold"
								android:textSize="18sp"
								android:layout_x="240px" 
								android:layout_y="277px" 
								android:layout_height="70px">
							</ImageButton>
							<ImageButton 
								android:layout_height="50px" 
								android:layout_x="25px" 
								android:id="@+id/seven" 
								android:text="7" 
								android:textStyle="bold"
								android:textSize="18sp"
								android:layout_width="90px" 
								android:layout_y="175px"
								android:background="@drawable/sevengrey">
							</ImageButton>
							<ImageButton 
								android:layout_height="50px" 
								android:text="8" 
								android:id="@+id/eight" 
								android:textSize="18sp"
								android:textStyle="bold"
								android:layout_width="90px" 
								android:layout_x="117.5px" 
								android:layout_y="175px"
								android:background="@drawable/eightgrey">
							</ImageButton>
							<ImageButton 
								android:layout_height="50px" 
								android:id="@+id/nine" 
								android:text="9" 
								android:textStyle="bold"
								android:textSize="18sp"
								android:layout_width="90px" 
								android:layout_x="210px" 
								android:layout_y="175px"
								android:background="@drawable/ninegrey">
							</ImageButton>
							<ImageButton 
								android:layout_height="50px" 
								android:id="@+id/star" 
								android:text="*" 
								android:textStyle="bold"
								android:textSize="18sp"
								android:layout_x="25px" 
								android:layout_width="90px" 
								android:layout_y="225px"
								android:background="@drawable/star">
							</ImageButton>
							<ImageButton 
								android:layout_height="50px" 
								android:id="@+id/zero" 
								android:text="0"
								android:textStyle="bold"
								android:textSize="18sp" 
								android:layout_width="90px" 
								android:layout_x="117.5px" 
								android:layout_y="225px"
								android:background="@drawable/zerogrey">
							</ImageButton>
							<ImageButton 
								android:layout_height="50px" 
								android:id="@+id/six" 
								android:text="6" 
								android:textStyle="bold"
								android:layout_width="90px" 
								android:textSize="18sp"
								android:layout_x="210px" 
								android:layout_y="125px"
								android:background="@drawable/sixgrey">
							</ImageButton>
							<ImageButton 
								android:layout_height="50px" 
								android:text="5"
								android:textStyle="bold"
								android:textSize="18sp" 
								android:id="@+id/five" 
								android:layout_width="90px" 
								android:layout_x="117.5px" 
								android:layout_y="125px"
								android:background="@drawable/fivegrey">
							</ImageButton>
							<ImageButton 
								android:layout_height="50px" 
								android:id="@+id/hash" 
								android:text="#"
								android:textStyle="bold"
								android:textSize="18sp" 
								android:layout_width="90px" 
								android:layout_x="210px" 
								android:layout_y="225px"
								android:background="@drawable/hashgrey">
							</ImageButton>
							<ImageButton 
								android:layout_height="50px" 
								android:layout_x="25px" 
								android:id="@+id/four" 
								android:textStyle="bold"
								android:text="4" 
								android:textSize="18sp"
								android:layout_width="90px" 
								android:layout_y="125px"
								android:background="@drawable/fourgrey">
							</ImageButton>
							<ImageButton 
								android:layout_height="50px" 
								android:layout_x="25px" 
								android:id="@+id/one"
								android:background="@drawable/onegrey"
								android:text="1"
								android:textStyle="bold"
								android:textSize="18sp"
								android:layout_width="90px" 
								android:layout_y="75px">
							</ImageButton>
							<ImageButton 
								android:layout_height="50px" 
								android:text="2"
								android:textSize="18sp" 
								android:id="@+id/two" 
								android:textStyle="bold"
								android:layout_width="90px" 
								android:layout_x="117.5px" 
								android:layout_y="75px"
								android:background="@drawable/twogrey">
							</ImageButton>
							<ImageButton 
								android:layout_height="50px" 
								android:id="@+id/three" 
								android:text="3"
								android:textStyle="bold"
								android:textSize="18sp" 
								android:layout_width="90px" 
								android:layout_x="210px" 
								android:layout_y="75px"
								android:background="@drawable/threegrey">
							</ImageButton>
							<EditText 
								android:layout_x="25px" 
								android:id="@+id/numberbox" 
								android:layout_height="60px" 
								android:layout_y="10px" 
								android:textSize="30sp"
								android:textStyle="bold"
								android:layout_width="275px">
							</EditText>
						</AbsoluteLayout>		       			            
            </FrameLayout>
</TabHost>